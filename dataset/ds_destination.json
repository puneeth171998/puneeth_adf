{
	"name": "ds_destination",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"companyName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().companyName,formatDateTime(utcNow(), 'yyyymmdd_hhMMss'),'.json')",
					"type": "Expression"
				},
				"container": "puneeth"
			}
		},
		"schema": {
			"type": "object",
			"properties": {
				"file": {
					"type": "string"
				},
				"total_errors": {
					"type": "integer"
				},
				"duplicate_rows": {
					"type": "integer"
				},
				"missing_columns": {
					"type": "integer"
				},
				"timestamp": {
					"type": "string"
				}
			}
		}
	}
}